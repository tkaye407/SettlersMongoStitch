  pod 'StitchSDK/StitchTwilioService', '~> 4.0.5'

  target 'SettlersTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SettlersUITests' do
    inherit! :search_paths
    # Pods for testing
  end

